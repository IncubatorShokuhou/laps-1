#!@CSH@

setenv LAPS_ROOT $1
setenv LAPS_BIN  $2/bin
setenv LAPS_DATA_ROOT $3
setenv RUN $4
setenv BUILD_DIR $5
setenv PLATFORM $6

echo " "
echo "List of file update times and building errors..."

setenv TZ GMT
cd $1

echo " "
echo "checking executables - should be 32 (counting lapsplot.exe)"
cd $LAPS_BIN
ls -lrt *.x *.exe
echo " Actual number is... `ls -l *.x *.exe | wc -l`"

echo " "
echo "checking make.out"
setenv FILE $1/make.out
grep -i " error " $FILE ; grep -i "^error " $FILE
grep '(E)' $FILE
grep '(S)' $FILE
grep -i undefined $FILE
grep -i stop $FILE
grep -i warning $FILE
grep -i "No rule" $FILE
ls -l $FILE

echo " "
echo "checking make_install.out"
setenv FILE $1/make_install.out
grep -i Error $FILE
grep '(E)' $FILE
grep '(S)' $FILE
ls -l $FILE

echo " "
echo "checking install_laps_builds.out"
grep build_laps.pl         $BUILD_DIR/install_laps_builds_$PLATFORM*
#tail -1                   $BUILD_DIR/build.log*
ls -l                      $BUILD_DIR/install_laps_builds_$PLATFORM*

echo " "
echo "checking static.nest7grid"
ls -l $LAPS_DATA_ROOT/static/static.nest7grid

echo " "
echo "checking localization output"
grep -i error            $3/log/laps_localization.log
grep -i warning          $3/log/laps_localization.log
grep -i cannot           $3/log/laps_localization.log
grep -i exceeds          $3/log/laps_localization.log
tail -1                  $3/log/laps_localization.log
ls -l                    $3/log/laps_localization.log

