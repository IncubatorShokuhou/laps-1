    $a9time_wgi =~ /(\d\d\d\d\d\d\d\d\d)/;
    $a9time_wgi = $1;

    my $dot = ".";

    foreach $source (@sources) {
        $sounding = 0;

        if ("$source" eq "what-got-in" ) {                                                                  # What-got-in stuff
            if ($n_sources == 1) {
                $wgi_only = 1;
            }

