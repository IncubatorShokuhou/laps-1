            if ($out_fields =~ "cld_" || $out_fields =~ "CLD_" || $out_fields =~ "ref" || $out_fields =~ "REF" || $out_fields =~ "sat_" || $out_fields =~ "SAT_") {    # Cloud fields
              $wgi_file = "$laps_data_root/log/cloud\.wgi\.$a9time_wgi";
