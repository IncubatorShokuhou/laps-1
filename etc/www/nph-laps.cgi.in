my $xcen_safe;
$xcen =~ /(.*)/;
$xcen_safe = $1;

my $ycen_safe;
$ycen =~ /(.*)/;
$ycen_safe = $1;

my $npix_safe;
$npix =~ /(.*)/;
$npix_safe = $1;

my $zoom_safe;
$zoom =~ /(.*)/;
$zoom_safe = $1;

my $zoom_lapsplot;

my $density_safe;
$density =~ /(.*)/;
$density_safe = $1;

my $thickness_safe;
$thickness =~ /(.*)/;
$thickness_safe = $1;

chomp $a9time;

my $a9time_safe;
$a9time =~ /(.*)/;
$a9time_safe = $1;

#my $a9time_wgi_safe;
my $wgi_file;
my $wgi_file2;
my $wgi_only = 0;

#print "xcen = $xcen $xcen_safe<br>";

#print "domain (safe) = $domain_safe<br>";
