# Used by configure to create makefile.inc 
# 
#
INSTALLROOT=@prefix@
DATAROOT=@datadir@
CC=@CC@

CFLAGS=@CFLAGS@ $(INC) @OPTIMIZE@ 
DBCFLAGS=@CFLAGS@ $(INC) -g -C -DDEBUG=1

FC       = @FC@
#
# NCAR graphics is used by lapsplot only.
#
NCARGFC  = @NCARGFC@

FFLAGS = @FFLAGS@ $(INC) @OPTIMIZE@
DBFLAGS = @FFLAGS@ $(INC) -g -C
# Define pre-processor command line options for .F files.

CPP = @CPP@
CPPFLAGS = @CPPFLAGS@
@USECPP@
# Define include paths.
NETCDF=@NETCDF@
INC = -I$(LAPSROOT)/src/include -I$(NETCDF)/include

AR  = @ARCHIVE@
RANLIB   = @RANLIB@
INSTALL=@INSTALL@
PERL = @PERL@
LIBPATHFLAG=-Wl,-L
OTHERLIBS = $(LIBPATHFLAG)$(NETCDF)/lib -lnetcdf  @OTHERLIBS@
