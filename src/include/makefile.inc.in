# Used by configure to create makefile.inc 
# 
#
INSTALLROOT=@prefix@
DATAROOT=@datadir@
CC=@CC@

CFLAGS=@CFLAGS@  @OPTIMIZE@ 
DBCFLAGS=@CFLAGS@ -g -C -DDEBUG=1

FC       = @FC@
#
# NCAR graphics is used by lapsplot only.
#
NCARGFC  = @NCARGFC@

FFLAGS = @FFLAGS@ $(INC) @OPTIMIZE@
DBFLAGS = @FFLAGS@ $(INC) -g -C
# Define pre-processor command line options for .F files.

CPP = @CPP@
CPPFLAGS = $(INC) @CPPFLAGS@
@USECPP@
# Define include paths.
NETCDF=@NETCDF@
INC = -I$(LAPSROOT)/src/include -I$(NETCDF)/include

AR  = @ARCHIVE@
RANLIB   = @RANLIB@
INSTALL=@INSTALL@
PERL = @PERL@
LIBPATHFLAG=@LIBPATHFLAG@

LAPSLIB=$(LIBPATHFLAG)$(LAPSROOT)/src/lib -llaps
DBLAPSLIB=$(LIBPATHFLAG)$(LAPSROOT)/src/lib -ldebuglaps

MTHERMOLIB=$(LIBPATHFLAG)$(LAPSROOT)/src/lib/mthermo -lmthermo
DBMTHERMOLIB=$(LIBPATHFLAG)$(LAPSROOT)/src/lib/mthermo -ldebugmthermo

UTILLIB=$(LIBPATHFLAG)$(LAPSROOT)/src/lib/util -lutil
DBUTILLIB=$(LIBPATHFLAG)$(LAPSROOT)/src/lib/util -ldebugutil





OTHERLIBS = $(LIBPATHFLAG)$(NETCDF)/lib -lnetcdf  @OTHERLIBS@
